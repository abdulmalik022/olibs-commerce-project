{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ncplFactory001"
		},
		"storageService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'storageService'"
		},
		"customersServer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/abdulmalik022/olibs-commerce-project/main/olist_customers_dataset.csv"
		},
		"sourceService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/abdulmalik022/olibs-commerce-project/main/"
		},
		"storageService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://firstproject001.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ncplFactory001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"storageService_accountKey": {
						"value": "[parameters('storageService_accountKey')]"
					},
					"customersServer_properties_typeProperties_url": {
						"value": "[parameters('customersServer_properties_typeProperties_url')]"
					},
					"sourceService_properties_typeProperties_url": {
						"value": "[parameters('sourceService_properties_typeProperties_url')]"
					},
					"storageService_properties_typeProperties_url": {
						"value": "[parameters('storageService_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}